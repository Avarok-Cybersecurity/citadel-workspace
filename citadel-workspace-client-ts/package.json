{
  "name": "citadel-workspace-client-ts",
  "version": "1.0.0",
  "description": "TypeScript client wrapper for Citadel Workspace with auto-generated types",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@avarok/citadel-protocol-types": "^0.13.1",
    "citadel-internal-service-wasm-client": "file:../citadel-internal-service/typescript-client"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
