// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider    = "cargo prisma"
  output      = "../src/db.rs"
  module_path = "crate:db"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Sessions {
  id  String @id
  cid String
}
